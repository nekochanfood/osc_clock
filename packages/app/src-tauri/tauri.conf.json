{
  "$schema": "https://schema.tauri.app/config/2",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "height": 600,
        "label": "main",
        "title": "OSC Clock",
        "width": 800
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "bun run vite build",
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../build"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "identifier": "net.chanfoo.osc-clock",
  "productName": "OSC Clock",
  "version": "3.0.0"
}